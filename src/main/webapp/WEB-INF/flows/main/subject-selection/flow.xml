<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <var name="subjectChoice" class="pl.agh.enrollme.webflow.model.SubjectChoice"/>

    <view-state id="view" model="subjectChoice">
        <transition on="done" to="subjectsChoosen"/>
    </view-state>

    <end-state id="subjectsChoosen">
        <output name="subjectChoice" value="subjectChoice" type="pl.agh.enrollme.webflow.model.SubjectChoice" />
    </end-state>

</flow>